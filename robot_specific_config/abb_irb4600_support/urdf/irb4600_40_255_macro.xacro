<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro">
  <xacro:macro name="abb_irb4600_40_255" params="prefix">
    <xacro:include filename="$(find abb_resources)/urdf/common_materials.xacro"/>
    <xacro:include filename="$(find abb_resources)/urdf/inertial_macro.xacro"/>
    <!--
      All inertial values for each of the robot's links below are retrieved/estimated from Solidworks
    -->
    <!-- link list -->
    <link name="${prefix}base_link">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600/collision/base_link.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600/visual/base_link.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.275" length="0.18" mass="30.28">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_1">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600/collision/link_1.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600/visual/link_1.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.23" length="0.48" mass="42.41">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_2">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_2.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_2.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.06" length="1.32" mass="34.32">
        <origin xyz="0 0 0.65" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_3">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_3.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_3.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.175" length="0.52" mass="51.16">
        <origin xyz="0 0 0.175" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_4">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_4.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_4.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.08" length="1.03" mass="13.95">
        <origin xyz="0.8 0 0" rpy="0 ${80} 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_5">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_5.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_5.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.08" length="0.23" mass="1.5">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <link name="${prefix}link_6">
      <collision name="collision">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/collision/link_6.stl"/>
        </geometry>
        <xacro:material_abb_yellow />
      </collision>
      <visual name="visual">
        <geometry>
          <mesh filename="package://abb_irb4600_support/meshes/irb4600_40_255/visual/link_6.dae"/>
        </geometry>
        <xacro:material_abb_graphite_white/>
      </visual>
      <xacro:cylinder_inertial radius="0.02" length="0.06" mass="0.27">
        <origin xyz="0 0 0" rpy="0 0 0" />
      </xacro:cylinder_inertial>
    </link>
    <!-- end of link list -->

    <!-- joint list -->
    <joint type="revolute" name="${prefix}joint_1">
      <origin xyz="0 0 0.495" rpy="0 0 0"/>
      <axis xyz="0 0 1"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}link_1"/>
      <!-- effort = -1 is interpreted as the default max effort, 
      on the otherhand 0 max effort will prevent effort ouputs from reaching the virtual joint -->
      <limit effort="-1" lower="${radians(-180)}" upper="${radians(180)}" velocity="${radians(175)}"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_2">
      <origin xyz="0.175 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_1"/>
      <child link="${prefix}link_2"/>
      <limit effort="-1" lower="${radians(-90)}" upper="${radians(150)}" velocity="${radians(175)}"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_3">
      <origin xyz="0 0 1.095" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_2"/>
      <child link="${prefix}link_3"/>
      <limit effort="-1" lower="${radians(-180)}" upper="${radians(75)}" velocity="${radians(175)}"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_4">
      <origin xyz="0 0 0.175" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <parent link="${prefix}link_3"/>
      <child link="${prefix}link_4"/>
      <limit effort="-1" lower="${radians(-400)}" upper="${radians(400)}" velocity="${radians(250)}"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_5">
      <origin xyz="1.270 0 0" rpy="0 0 0"/>
      <axis xyz="0 1 0"/>
      <parent link="${prefix}link_4"/>
      <child link="${prefix}link_5"/>
      <limit effort="-1" lower="${radians(-125)}" upper="${radians(120)}" velocity="${radians(250)}"/>
    </joint>
    <joint type="revolute" name="${prefix}joint_6">
      <origin xyz="0.135 0 0" rpy="0 0 0"/>
      <axis xyz="1 0 0"/>
      <parent link="${prefix}link_5"/>
      <child link="${prefix}link_6"/>
      <limit effort="-1" lower="${radians(-400)}" upper="${radians(400)}" velocity="${radians(360)}"/>
    </joint>
    <!-- end of joint list -->

    <!-- ROS-Industrial 'base' frame- base_link to ABB World Coordinates transform -->
    <link name="${prefix}base" />
    <joint name="${prefix}base_link-base" type="fixed">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}base_link"/>
      <child link="${prefix}base"/>
    </joint>

    <!-- ROS-Industrial 'flange' frame- attachment point for EEF models -->
    <link name="${prefix}flange"/>
    <joint type="fixed" name="${prefix}joint_6-flange">
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <parent link="${prefix}link_6"/>
      <child link="${prefix}flange"/>
    </joint>

    <!-- ROS-Industrial 'tool0' frame- all-zeros tool frame -->
    <link name="${prefix}tool0" />
    <joint name="${prefix}link_6-tool0" type="fixed">
      <origin xyz="0 0 0" rpy="0 ${radians(90)} 0" />
      <parent link="${prefix}flange" />
      <child link="${prefix}tool0" />
    </joint>
  </xacro:macro>
</robot>
